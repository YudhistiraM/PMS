  <script src="/javascripts/ie-emulation-modes-warning.js"></script>

  <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>

<body>

  <nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
      <div class="navbar-header">
        <!-- <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button> -->
      <a class="navbar-brand">Project Management System</a>
    </div>
    <div id="navbar" class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
        <li><a href="/project">Project</a></li>
        <li><a href="/profile"> Profile</a></li>
      </ul>
      <div class="button">
        <a href="/logout" class="btn btn-link navbar-btn navbar-right">Sign out</a>
      </div><!--/.nav-collapse -->
    </div>
  </nav>


  <div class="container">
    <div class="panel-heading col-lg-12">
      <h1><b>Update Project</b></h1><hr>
    </div>
    <form class="" action="" method="post">
      <div class="panel-body">

        <div class="row">
          <div class="col-md-2">
            <div class="checkbox">
              <label>Name</label>
            </div>
          </div>
          <div class="col-md-8">
             <input type="text" class="form-control" id="name" name="name" placeholder="Name Project" value="<%= data[0].name%>">
          </div>
        </div>

        <div class="row">
          <div class="col-md-2">
            <div class="checkbox">
              <label>Member</label>
            </div>
          </div>
          <div class="checkbox col-md-10">
            <label>
              <% for (let i = 0; i < data1.length; i++) { %>
              <input type="checkbox" name="memberproject" value="<%= data1[i].userid%>"<%= util.cekMember(data1[i].userid, data) ? 'checked' : '' %>><%= data1[i].fullname%></br>
              <% } %>
            </label>
          </div>
        </div>

        <div class="row">
          <div class="col-md-1" align="center"></br>
            <button type="submit" class="btn btn-primary">Update</button>
          </div>
        </div>

      </form>
    </div>
  </div><!-- /.container -->
</body>
</html>
