<div class="container-fluid">
  <div class="row">
    <div class="col-sm-3 col-md-2 sidebar">
      <ul class="nav nav-sidebar">
        <li><a href="/project_overview/<%= projectid %>"><b>Overview</b></a></li>
        <li><a href="/project_activity/<%= projectid %>"><b>Activity</b></a></li>
      </ul>
      <ul class="nav nav-sidebar">
        <li class="active"><a href="/project_members/<%= projectid %>"><b>Members</b></a></li>
        <li><a href="/project_issues/<%= projectid %>"><b>Issues</b></a></li>
      </ul>

    </div>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
      <h3 class="page-header"><b>Add Members</b></h3>
        <div class="panel-body">

          <form class="" action="" method="post">

              <div class="row">
                <div class="col-md-2">
                  <div class="checkbox">
                    <label>Members Project</label>
                  </div>
                </div>
                <div class="col-md-10">
                  <select class="form-control" name="member">
                    <option>Chosee Members...</option>
                    <% for (let i = 0; i < dataUsers.length; i++) { %>
                      <option name="member" value="<%= dataUsers[i].userid %>"><%= dataUsers[i].fullname%></option>
                      <% } %>
                    </select>
                  </div>
              </div>

            <div class="row">
              <div class="col-md-2">
                <div class="checkbox">
                  <label>Position</label>
                </div>
              </div>
              <div class="checkbox">
                <label>
                  <input type="radio" name="role" value="Manager"> Manager
                  <br><input type="radio" name="role" value="Software Developer" > Software Developer
                  <br><input type="radio" name="role" value="Quality Assurance" > Quality Assurance
                </label>
              </div>
            </div>

            <div class="row">
              <div class="col-md-1" align="center">
                <button type="submit" class="btn btn-primary">Add</button>
              </div>
            </div>

        </form>



      </div>
    </div>
  </div>

