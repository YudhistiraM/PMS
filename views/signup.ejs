<div class="container">
  <div class="panel-heading col-lg-12">
    <h1><b>SignUp</b></h1><hr>
  </div>
  <div class="panel-body">
    <form class="form-horizontal" method="post">

      <div class="form-group row">
        <div class="checkbox col-md-2">
          <label>First Name</label>
        </div>
        <div class="col-md-8">
          <input type="text" class="form-control" id="firstname" name="firstname" placeholder="Your First Name" required>
        </div>
      </div>


      <div class="form-group row">
        <div class="checkbox col-md-2">
          <label>Last Name</label>
        </div>
        <div class="col-md-8">
          <input type="text" class="form-control" id="lastname" name="lastname" placeholder="Your Last Name" required>
        </div>
      </div>

      <div class="form-group row">
        <div class="checkbox col-md-2">
          <label>Email</label>
        </div>
        <div class="col-md-8">
          <input type="text" class="form-control" id="email" name="email" placeholder="Email" required>
        </div>
      </div>

      <div class="form-group row">
        <div class="checkbox col-md-2">
          <label>password</label>
        </div>
        <div class="col-md-8">
          <input type="text" class="form-control" id="password" name="password" placeholder="Password" required>
        </div>
      </div>

      <div class="form-group row">
        <div class="col-md-12" align="left">
          <button type="submit" class="btn btn-primary">Submit</button><hr>
        </div>
      </div>

    </form>
  </div>
